{
    "name": "uni-sport-berlin",
    "version": "1.0.0",
    "dependencies": {
      "@prisma/client": "^5.4.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2",
      "@testing-library/jest-dom": "^5.17.0",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "web-vitals": "^2.1.4"
    },
    "devDependencies": {
      "@types/cors": "^2.8.15",
      "@types/express": "^4.17.20",
      "@types/node": "^20.8.7",
      "prisma": "^5.5.1",
      "ts-node-dev": "^2.0.0",
      "react-scripts": "5.0.1"
    },
    "workspaces": [
      "frontend",
      "backend"
    ],
    "scripts": {
      "start": "npm run start-frontend && npm run start-backend",
      "start-frontend": "cd frontend && npm start",
      "start-backend": "npm cd backend && npm start",
      "build": "npm run build-frontend && npm run build-backend",
      "build-frontend": "cd frontend && npm run build",
      "build-backend": "cd backend && npm run build",
      "deploy": "npm run deploy-frontend && npm run deploy-backend",
      "deploy-frontend": "cd frontend && npm run deploy",
      "deploy-backend": "cd backend && npm run deploy"
    }
  }
  